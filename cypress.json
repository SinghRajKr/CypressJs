{
    "baseUrl": "https://shore-qa.otaliodev.com",
    "defaultCommandTimout": 15000,
    "blacklistHosts": "*google.com",
    "env": {
        "projectId": "Otalio",
        "EMAIL": "medha@otalio.com",
        "PASSWORD": "888"
    },
    "trashAssetsBeforeRuns": true,
    "viewportHeight": 660,
    "waitForAnimations": true,
